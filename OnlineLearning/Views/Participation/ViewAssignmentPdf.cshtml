@model OnlineLearning.Models.AssignmentModel

@{
    ViewBag.Title = "View PDF";
}



@if (!string.IsNullOrEmpty(Model?.AssignmentLink))
{
    <iframe src="~/Assignment/@Model.AssignmentLink" width="100%" height="100%" style="border: none;"></iframe>
}
else
{
    <p>No PDF available to display.</p>
}
